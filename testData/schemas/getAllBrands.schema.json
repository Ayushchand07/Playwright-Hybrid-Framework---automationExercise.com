{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Get All Brands Schema",
  "type": "object",
  "additionalProperties": false,
  "required": ["responseCode", "brands"],
  "properties": {
    "responseCode": {
      "type": "integer",
      "enum": [200]
    },
    "brands": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["id", "brand"],
        "properties": {
          "id": {
            "type": "integer",
            "minimum": 1
          },
          "brand": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    }
  }
}
